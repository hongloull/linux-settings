<filetype binary="false" description="usd format" name="USD">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
    </options>
    <keywords keywords="add;asset;bool;color3;def;float;float3;int;reference;rel;string;token;uniform" ignore_case="false" />
    <keywords2 keywords="Look;Mesh;Scope;Xform" />
  </highlighting>
  <extensionMap>
    <mapping pattern=".usd" />
    <mapping pattern=".usda" />
  </extensionMap>
</filetype>